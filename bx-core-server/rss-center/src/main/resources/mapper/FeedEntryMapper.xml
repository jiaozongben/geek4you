<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gk4u.rss.backend.mapper.FeedEntryMapper">
    <select id="findEntryExists" resultType="int" parameterType="com.gk4u.rss.backend.entity.FeedEntry">
      SELECT count(1) FROM `feed_entry` where url = #{ url,jdbcType=VARCHAR}
    </select>
</mapper>
